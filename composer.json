{
    "version": "6.1.0",
    "name": "elnebuloso\/phing-commons",
    "type": "library",
    "license": "MIT",
    "description": "phing commons build stack",
    "keywords": [
        "phing",
        "build management",
        "continuous integration"
    ],
    "authors": [
        {
            "name": "Jeff Tunessen",
            "email": "jeff.tunessen@gmail.com",
            "role": "Maintainer"
        }
    ],
    "autoload": {
        "psr-0": {
            "Commons_": "src\/"
        }
    },
    "repositories": [
        {
            "type": "pear",
            "url": "http:\/\/pear.phpunit.de"
        },
        {
            "type": "pear",
            "url": "http:\/\/pear.symfony.com"
        },
        {
            "type": "pear",
            "url": "http:\/\/pear.netpirates.net"
        },
        {
            "type": "pear",
            "url": "http:\/\/pear.pdepend.org"
        },
        {
            "type": "pear",
            "url": "http:\/\/pear.phpmd.org"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "phing\/phing": "2.7.0",
        "phpunit\/phpunit": "3.7.37",
        "apigen\/apigen": "2.8.0",
        "nette\/nette": "~2.1.3",
        "squizlabs\/php_codesniffer": "1.5.3",
        "phploc\/phploc": "2.0.6",
        "sebastian\/phpdcd": "1.0.2",
        "pear-pear.phpunit.de\/phpcpd": "2.0.0",
        "pear-pear.pdepend.org\/PHP_Depend": "1.1.4",
        "pear-pear.phpmd.org\/PHP_PMD": "1.5.0"
    },
    "config": {
        "bin-dir": "bin"
    }
}