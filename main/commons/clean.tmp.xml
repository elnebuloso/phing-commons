<?xml version="1.0" encoding="UTF-8"?>


<project default="clean.tmp">


    <property name="clean.tmp:description" value="Cleanup Project Temporary Folder"/>


    <target name="clean.tmp:help" depends="clean.tmp:init" hidden="true">
        <echo message="${clean.tmp:description}"/>
        <echo message=""/>
        <echo message="property             value"/>
        <echo message="----------------------------------------------------------------------------------------------------"/>
        <echo message="project.build.tmp    ${project.build.reports}"/>
    </target>


    <target name="clean.tmp:init" hidden="true">
        <echo message="no actions defined"/>
    </target>


    <target name="clean.tmp:before" hidden="true">
        <echo message="no actions defined"/>
    </target>


    <target name="clean.tmp:main" hidden="true">
        <delete dir="${project.build.tmp}"/>
        <mkdir dir="${project.build.tmp}"/>
    </target>


    <target name="clean.tmp:after" hidden="true">
        <echo message="no actions defined"/>
    </target>


    <target name="clean.tmp" depends="clean.tmp:init, clean.tmp:before, clean.tmp:main, clean.tmp:after">
        <echo message="successful"/>
    </target>


</project>